{
    "files.associations": {
        "tusb_compiler.h": "c",
        "types": "cpp",
        "unordered_map": "c",
        "unordered_set": "c",
        "units": "cpp",
        "freertos.h": "c",
        "queue.h": "c",
        "tusb_option.h": "c",
        "stm32f4xx_hal.h": "c",
        "stm32f4xx.h": "c"
    },
    "files.autoSave": "onFocusChange",
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.mergeConfigurations": true,
    "C_Cpp.default.includePath": [
        "include",
        "lib/FreeRTOS-Kernel/include",
        "lib/FreeRTOS-Kernel/portable/GCC/ARM_CM4F",
        "lib/etl/include",
        "lib/cmsis_device_f4/Include",
        "lib/nanopb",
        "lib/stm32f4xx_hal_driver/Inc",
        "lib/tinyusb/src",
        "lib/lwip/src/include",
        "lib/lwip/src/include/ipv4",
        "lib/lwip/src/include/lwip/apps",
        "lib/tinyusb/lib/networking"
    ],
    "C_Cpp.files.exclude": {
        "**/.vscode": true,
        "**/.vs": true
    },
    "cmake.configureArgs": [
        "-DCMAKE_VERBOSE_MAKEFILE=true",
        "-DCMAKE_BUILD_TYPE=Debug",
        "-DCMAKE_EXPORT_COMPILE_COMMANDS=1"
    ],
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools"
}