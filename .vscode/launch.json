{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/app/src/uNav2.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "configFiles": [ "${workspaceRoot}/tools/openocd.cfg" ],
            "device": "STM32F407VE",
            "interface": "swd",
            "runToMain": true, // else it starts at reset handler - not interested
            "preLaunchTask": "CMake: build", // configured in tasks.json
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            "svdFile": "", // Include svd to watch device peripherals
            "rtos": "FreeRTOS",
            "showDevDebugOutput": "parsed",
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "",
                        "port": 0,
                        "type": "console"
                    }
                ]
            },
        },
        {
            "name": "Attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/app/src/uNav2.elf",
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "configFiles": [ "${workspaceRoot}/tools/openocd.cfg" ],
            "device": "STM32F407VG",
            "interface": "swd",
            "preLaunchTask": "CMake: build", // configured in tasks.json
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            "svdFile": "", // Include svd to watch device peripherals
            "rtos": "FreeRTOS",
            "showDevDebugOutput": "parsed",
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "",
                        "port": 0,
                        "type": "console"
                    }
                ]
            },
        }
    ]
}